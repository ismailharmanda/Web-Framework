st templateElement = document.createElement(\"template\");\r\n    templateElement.innerHTML = this.template();\r\n    this.bindEvents(templateElement.content);\r\n    this.parent.append(templateElement.content);\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"a5dfa9d9213b8e76ec6ace98d1b43aa5","cacheData":{"env":{}}}